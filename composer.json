{
    "name": "abdelhamiderrahmouni/laravel-dailyco",
    "description": "[Unofficial] Laravel SDK for Daily.co",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LaravelDailyco\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LaravelDailyco\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbdelhamidErrahmouni\\LaravelDailyco\\ServiceProvider"
            ]
        }
    }
}
