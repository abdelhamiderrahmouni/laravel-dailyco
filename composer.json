{
    "name": "steadfastcollective/laravel-dailyco",
    "description": "Laravel SDK for Daily.co's REST API",
    "extra": {
        "laravel": {
            "providers": [
                "SteadfastCollective\\LaravelDailyco\\ServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SteadfastCollective\\LaravelDailyco\\Tests\\": "tests"
        }
