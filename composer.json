{
    "name": "abdelhamiderrahmouni/laravel-dailyco",
    "description": "[Unofficial] Laravel SDK for Daily.co",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "pestphp/pest": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LaravelDailyco\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LaravelDailyco\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbdelhamidErrahmouni\\LaravelDailyco\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test:unit": "vendor/bin/pest --testsuite=Unit",
        "test:feature": "vendor/bin/pest --testsuite=Feature",
        "test:coverage": "vendor/bin/pest --coverage"
    }
}
